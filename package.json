{
	"name": "discord-presence-night",
	"author": {
		"name": "night."
	},
	"displayName": "Discord Presence | night.",
	"description": "Visual Studio Code plugin for Beatiful Discord Presence by night.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.62.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:discord-presence-night.enable",
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "discord-presence-night.enable",
				"title": "Connect/Disconnect Presence"
			}
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/nightidk/discord-presence-night"
	},
	"bugs": {
		"url": "https://github.com/nightidk/discord-presence-night/issues"
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.62.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4",
		"@vscode/test-electron": "^1.6.2",
		"discord-rich-presence-typescript": "0.0.8"
	}
}
